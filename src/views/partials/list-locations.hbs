{{#each locations}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{title}}
    </h2>
    <p>{{category}}</p>
    <a href="/location/{{_id}}" class="button">
      <span class="icon is-small">
        <i class="fas fa-info-circle"></i>
      </span>
    </a>
    <a href="/dashboard/deletelocation/{{_id}}" class="button">
      <i class="fas fa-trash"></i>
    </a>
  </div>
{{/each}}